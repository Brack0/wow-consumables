{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": ["node_modules/@types"],
    "lib": ["es2016", "es2017", "dom"],
    "paths": {
      "@model": ["./app/shared/model.imports"],
      "@rxjs": ["./app/shared/rxjs.imports"],
      "@validators": ["./app/shared/validators.imports"]
    }
  }
}
